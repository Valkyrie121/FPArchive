{
    "name": "FP Archive",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" 
    },
    "version": "0.2",
    "description": "Checks URL and redirects as required.",
    "background": { 
       "page":"bg.html"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["http://forum.facepunch.com/*", "https://forum.facepunch.com/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "tabs"
    ]
}
